{
  "name": "web",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "supabase.commit":"supabase db remote commit",
    "supabase.types":"supabase gen types typescript --linked --schema  private  > ./src/@types/database/database.d.ts",
    "supabase.login":"supabase login && supabase link",
    "supabase.init":"supabase init",
    "supabase.start":"supabase start",
    "supabase.reset":"supabase stop --no-backup && supabase start",
    "supabase.restart":"supabase stop && supabase start",
    "supabase.stop":"supabase stop",
    "supabase.status":"supabase status",
    "prisma.push": "npx prisma db push --schema prisma/mongo/schema.prisma && npx prisma db push --schema prisma/postgres/schema.prisma",
    "prisma.generate": "prisma generate --schema prisma/postgres/schema.prisma && prisma generate --schema prisma/mongo/schema.prisma",
    "postgres.push": "npx prisma db push --schema prisma/postgres/schema.prisma",
    "postgres.generate": "prisma generate --schema prisma/postgres/schema.prisma",
    "postgres.pull": "npx prisma db pull --schema prisma/postgres/schema.prisma",
    "postgres.migrate.create": "prisma migrate dev --schema prisma/postgres/schema.prisma --create-only",
    "postgres.migrate": "prisma migrate dev --schema prisma/postgres/schema.prisma",
    "mongo.push": "npx prisma db push --schema prisma/mongo/schema.prisma",
    "mongo.generate": "prisma generate --schema prisma/mongo/schema.prisma"
  },
  "prisma": {
    "seed": "npx ts-node  --compilerOptions {\"module\":\"CommonJS\"}  prisma/postgres/seed.ts && npx ts-node  --compilerOptions {\"module\":\"CommonJS\"}  prisma/mongo/seed.ts",
    "seed.postgres": "npx ts-node  --compilerOptions {\"module\":\"CommonJS\"}  prisma/postgres/seed.ts",
    "seed.mongo": "npx ts-node  --compilerOptions {\"module\":\"CommonJS\"}  prisma/mongo/seed.ts"
  },
  "dependencies": {
    "@fec/remark-a11y-emoji": "3.1.0",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.4.12",
    "@prisma/client": "5.1.1",
    "@radix-ui/react-hover-card": "1.0.6",
    "@supabase/auth-helpers-nextjs": "0.7.4",
    "@supabase/supabase-js": "2.31.0",
    "@t3-oss/env-nextjs": "0.6.0",
    "@vercel/otel": "0.3.0",
    "next": "13.4.12",
    "next-mdx-remote": "4.4.1",
    "node-html-parser": "6.1.5",
    "octokit": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-collapse": "0.1.2",
    "remark-emoji": "3.1.2",
    "remark-gfm": "3.0.1",
    "swr": "2.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@myworldx/eslint-config": "workspace:*",
    "@octokit/webhooks": "12.0.3",
    "@octokit/webhooks-types": "7.1.0",
    "@types/mdx": "2.0.5",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.8",
    "postcss": "8.4.27",
    "prettier": "^3.0.0",
    "prisma": "5.1.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}
